@prefix : <urn:x:>.

:Cat a [
        a :Animal;
        ].

# Bind blank node [ a Animal ] tp ?X
{ :Cat a ?X. } => { ?X :contains :Felines }.

# Bounded blank node will be skolemized in the output
{ :Cat a ?Y. } => { ?Y :contains :HouseAnimals }.

# Bounded blank node will be skolemized in the output
{ ?Z :contains :Felines } => { ?Z :mayAlsoHave :Lions }.
